http {
        listen localhost:8080;
        autoindex on;

    server {
        upload_pass ./www/uploads;
		root ./www2;

        location /** {
            allow POST;
            allow GET;
            allow DELETE;
        }

        location  /typing/* {
			root ./www;
            allow GET;
			index index.html;
        }

        location /static1/* {
			root ./www3;
            allow POST;
            allow GET;
            allow DELETE;
			index index.html;
        }
    }
}